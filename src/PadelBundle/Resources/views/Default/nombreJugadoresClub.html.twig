{% extends '::base.html.twig' %}
{% block title %}Jugadores{% endblock %}
{% block body %}
 <h3>Jugadores del club</h3>
 <table class="table table-striped table-inverse">
   <tr>
     <th>Nombre</th>
     <th>Apellidos</th>
     <th>Sexo</th>
     <th>Edad</th>
     <th>Nivel</th>
     <th>mostrar</th>
   </tr>
   {% for jugadores in nombre %}
   <tr>
     <td>{{ jugadores.nombre }}</td>
     <td>{{ jugadores.apellidos }}</td>
     <td>{{ jugadores.sexo }}</td>
     <td>{{ jugadores.edad }}</td>
     <td>{{ jugadores.nivel }}</td>
     <td><a href="{{path(
      'padel_nombreId',
     { 'id': jugadores.id}
   )}} ">mostrar</a></td>
   </tr>
   {% endfor %}
 </table>
{% endblock %}
